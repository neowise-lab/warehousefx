<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="300.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_RIGHT" prefWidth="200.0" spacing="10.0">
         <children>
            <Label fx:id="title" maxWidth="1.7976931348623157E308" prefHeight="35.0" styleClass="title" text="Сменить базу данных" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets left="20.0" right="10.0" />
         </padding>
      </HBox>
      <VBox spacing="10.0">
         <children>
            <ListView fx:id="changeOptionList" fixedCellSize="42.0" prefHeight="90" />
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER_RIGHT" prefWidth="200.0" spacing="10.0" style="-fx-border-width: 1 0 0 0; -fx-border-color: #2D2D2D;">
         <children>
            <Button mnemonicParsing="false" onAction="#select" prefWidth="75.0" styleClass="green-button" text="Выбрать" />
            <Button layoutX="20.0" layoutY="10.0" minWidth="75.0" mnemonicParsing="false" onAction="#cancel" text="Отмена" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="15.0" right="20.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
